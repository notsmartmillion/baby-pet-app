{
  "expo": {
    "name": "Baby Pet",
    "slug": "baby-pet-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "automatic",
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.yourcompany.babypet",
      "infoPlist": {
        "NSPhotoLibraryUsageDescription": "We need access to your photo library to select pet photos for transformation.",
        "NSCameraUsageDescription": "We need access to your camera to take pet photos for transformation."
      }
    },
    "android": {
      "package": "com.yourcompany.babypet",
      "permissions": [
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE",
        "CAMERA"
      ]
    },
    "plugins": [
      "expo-router"
    ],
    "scheme": "babypet",
    "extra": {
      "apiUrl": "http://10.0.0.117:3000"
    }
  }
}

